{
  "name": "@bm-js/icons",
  "version": "0.0.0-semantically-released",
  "description": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "semantic-release": "semantic-release",
    "build": "npx tsc && node dist/compileIcons.js && npx tsc",
    "prepare-preview-icons": "rm -rf preview/public/svgSrc && cp -r svgSrc preview/public",
    "build-frontend": "npm run prepare-preview-icons && cd preview && npm run build",
    "frontend": "npm run prepare-preview-icons && cd preview && npm run dev"
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "publishConfig": {
    "access": "restricted"
  },
  "dependencies": {
    "react": "^18.2.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "semantic-release": "^23.0.2",
    "@types/uuid": "^9.0.8",
    "@types/node": "^20.11.19",
    "@types/react": "^18.2.57",
    "cheerio": "^1.0.0-rc.12",
    "typescript": "^5.3.3"
  }
}
